{
  "name": "mas-socket",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest",
    "@types/moment-timezone": "^0.5.13",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "express": "^5.2.1",
    "prettier": "^3.7.4",
    "typescript-eslint": "^8.51.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "_moduleAliases": {
    "@": "src/",
    "@@": "."
  },
  "scripts": {
    "start": "bun run ./src/index.ts",
    "test:server": "bun run ./test/server.ts",
    "build:client": "bun build ./src/client-browser.ts --target browser --format esm --outfile ./dist/client/index.js",
    "build:client:iife": "bun build ./src/client-browser.ts --target browser --format iife --outfile ./dist/client/index.iife.js",
    "build:server": "bun build ./src/server.ts --target node --format esm --outfile ./dist/server/index.js",
    "build": "bun run build:client && bun run build:client:iife && bun run build:server"
  },
  "dependencies": {
    "@types/express": "^5.0.6",
    "@types/lodash": "^4.17.21",
    "@types/ws": "^8.18.1",
    "ansi-colors": "^4.1.3",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "moment-timezone": "^0.6.0",
    "ws": "^8.19.0"
  }
}